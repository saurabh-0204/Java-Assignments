class P3 {

	public static void main(String[] args) {
		
		
		int rows =5;
		
		System.out.println("Printing Plus Pattern of Stars");
		
		for (int i = 1 ; i <= rows * 2 - 1; i++ ) 
		{
			if (i != rows) 
			{
				for (int j = 1 ; j <= rows; j++ ) 
				{
					if(j == rows) {
						System.out.print("  * ");
					}
					System.out.print(" ");
				}
			}
			else 
			{	
				for (int k = 1 ; k <= rows * 2 - 1; k++ ) 
				{
					System.out.print("  * ");
				}
			}
			System.out.println();
		}
	}
}