package random;

public class FibboRecursion {
	public static void main(String[] args) {
		System.out.println("Fibbonacy Series");
		printFibbonacy(10);
	}

	private static void printFibbonacy(int i) {
		int a = 0;
		int b = 1;
		int c = 1;
		if (i > 1) {
			System.out.println(a);
			a = b;
			b = c;
			c = a + b;
			printFibbonacy(i-1)+printFibbonacy(i-2);
		}
	}
}
